{
  "name": "plusone-event-crew",
  "private": true,
  "version": "1.0.0",
  "description": "Find friends for local events - Never go to shows, concerts, or activities alone again",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS=--max-old-space-size=4096 vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "ingest:tm": "node --enable-source-maps ./scripts/ingest_ticketmaster_events.mjs",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:verify": "npm run build && node scripts/security-audit.js",
    "security:audit": "node scripts/security-audit.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup": "npm install && npm run backend:install",
    "verify:photos": "node verify-photo-integration.js",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "ios:build": "npm run build && npx cap sync ios",
    "ios:fix-deps": "./scripts/fix-ios-dependencies.sh",
    "ios:sync": "./scripts/build-and-sync-ios.sh",
    "ios:archive": "./scripts/build-and-upload-ios.sh --skip-upload",
    "ios:upload": "./scripts/build-and-upload-ios.sh",
    "android:build": "npm run build && npx cap sync android",
    "sync-admin": "./scripts/sync-admin-to-getsynth.sh",
    "push:worker": "node backend/push-notification-worker.js"
  },
